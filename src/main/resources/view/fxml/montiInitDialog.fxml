      	<Label text="Package Name" GridPane.columnIndex="0" GridPane.rowIndex="2" />
      	<Label text="Name" GridPane.columnIndex="0" GridPane.rowIndex="3" />
      	<Label text="Generics/TypeParameter" GridPane.columnIndex="0" GridPane.rowIndex="4" />
      	<Label text="Configuration Parameter" GridPane.columnIndex="0" GridPane.rowIndex="5" />
      	<TextField fx:id="importTextField" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      	<TextField fx:id="packageTextField" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      	<TextField fx:id="nameTextField" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
