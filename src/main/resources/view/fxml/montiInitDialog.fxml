    <HBox id="HBox" alignment="CENTER" minWidth="-1.0" spacing="5.0" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0">
      <children>
        <Button fx:id="okButton" mnemonicParsing="false" prefWidth="80.0" text="OK" onAction="#handleOk" />
        <Button fx:id="cancelButton" mnemonicParsing="false" prefWidth="80.0" text="Cancel" onAction="#handleCancel" />
      </children>
    </HBox>    
   </children>
</AnchorPane>